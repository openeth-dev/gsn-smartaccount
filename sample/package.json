{
  "name": "safeaccount-sample",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "mocha": "mocha --require esm --recursive test --exit",
    "test": "npx run-with-testrpc -d -a 15 -e 1e6 -g 1e8 -l 1e12 --allowUnlimitedContractSize 'yarn mocha'",
    "ci": "yarn run lint && yarn test",
    "lint": "yarn run lint:js",
    "lint:fix": "yarn run lint:js:fix",
    "lint:js": "eslint -f unix --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ignore-path .gitignore . --fix",
    "react-start": "concurrently --kill-others 'yarn build-account-frame-js -w' 'react-scripts start'",
    "react-build": "PROD=1 yarn build-account-frame-js && react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "build-account-frame-js": "webpack-cli --config ./account.webpack.js ",
    "ganache": "npx ganache-cli -d -a 15 -e 1e6 -g 1e8 -l 1e12 --allowUnlimitedContractSize",
    "testserver": "npx run-with-testrpc -d -a 15 -e 1e6 -g 1e8 -l 1e12 --allowUnlimitedContractSize 'yarn justtestserver'",
    "justtestserver": "node -r esm ./scripts/runtestserver.js"
  },
  "repository": "https://github.com/tabookey/sc-sample.git",
  "author": "Dror Tirosh <dror@tabookey.com>",
  "license": "MIT",
  "dependencies": {
    "@truffle/contract": "latest",
    "abi-decoder": "^2.2.2",
    "async-await-foreach": "^1.0.4",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "eslint-plugin-react": "^7.17.0",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^6.2.0",
    "ethereumjs-wallet": "^0.6.3",
    "express": "^4.17.1",
    "google-auth-library": "^5.6.1",
    "gsn-sponsor": "0.0.3-beta.8",
    "jsonrpc-lite": "^2.1.0",
    "localgsn": "^1.0.0-beta-0.0",
    "phone": "^2.4.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "safechannels-contracts": "file:../solidity/",
    "tabookey-gasless": "^0.4.4",
    "web3": "1.2.4",
    "web3-utils": "1.2.1",
    "webpack": "4.41.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "mocha": "^6.2.2",
    "run-with-testrpc": "0.3.1",
    "sinon": "^7.5.0",
    "standard": "^14.3.1",
    "webpack-cli": "^3.3.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
